Public Function String2B70(charstr As String) As Double

Dim leng As Integer
leng = Len(charstr)
Dim i As Integer
Dim char As String
String2B70 = 0
For i = 1 To leng
    char = Mid(charstr, i, 1)
    String2B70 = String2B70 + (AlphaNumericEncrypt.AlphaNumeric2Num(char) * (70 ^ (leng - i)))
Next i
End Function

Public Function B70toString(b70num As Double) As String
Dim reme As Double
Dim num As Double
Dim comp As Double
Dim i As Integer
Dim j As Integer
comp = 0
i = 0
Do While comp < b70num
    comp = 70 ^ i
    i = i + 1
Loop
i = i - 2
    
For j = 0 To i
    comp = b70num / (70 ^ (i - j))
    reme = Int(comp)
    B70toString = B70toString & AlphaNumericEncrypt.Num2AlphaNumeric(reme)
    b70num = b70num - reme * (70 ^ (i - j))
Next j

End Function
